  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "prettier.configPath": ".prettierrc.js",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "commentTranslate.targetLanguage": "en",
